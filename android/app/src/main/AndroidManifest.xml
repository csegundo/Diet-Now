<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dietnow.app.ucm.fdi">
    <!-- Permisos de la app -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dietnow"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DietNow">
        <!-- UserProfileEditActivity -->
        <activity
            android:name=".UserProfileEditActivity"
            android:exported="false"
            android:label="@string/label_user_edit_profile" /> <!-- AllUserActivity -->
        <activity
            android:name=".AllUserActivity"
            android:exported="false"
            android:label="@string/label_all_user" /> <!-- AdminPageActivity -->
        <activity
            android:name=".AdminPageActivity"
            android:exported="false"
            android:label="@string/label_main_page" /> <!-- UserProfileActivity -->
        <activity
            android:name=".UserProfileActivity"
            android:exported="false"
            android:label="@string/label_user_profile" /> <!-- Register AdminActivity -->
        <activity
            android:name=".RegisterAdminActivity"
            android:exported="false"
            android:label="@string/label_register" /> <!-- UserPageActivity -->
        <activity
            android:name=".UserPageActivity"
            android:exported="false"
            android:label="@string/label_main_page" /> <!-- RegisterActivity -->
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:label="@string/label_register" /> <!-- LoginActivity -->
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/label_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>