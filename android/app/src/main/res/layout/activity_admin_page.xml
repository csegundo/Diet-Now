<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminPageActivity">

    <Button
        android:id="@+id/adminCrearDietaBtn"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Crear dieta"
        app:layout_constraintBottom_toTopOf="@+id/adminDietasPublicadasBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.911" />

    <Button
        android:id="@+id/adminPerfilBtn"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Mi perfil"
        app:layout_constraintBottom_toTopOf="@+id/adminCrearDietaBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.886" />

    <Button
        android:id="@+id/adminMisDietasBtn"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Mis dietas creadas"
        app:layout_constraintBottom_toTopOf="@+id/adminCrearUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.965" />

    <Button
        android:id="@+id/adminDietasPublicadasBtn"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Dietas publicadas"
        app:layout_constraintBottom_toTopOf="@+id/adminMisDietasBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.943" />

    <Button
        android:id="@+id/adminCrearUser"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Crear Usuario"
        app:layout_constraintBottom_toTopOf="@+id/adminModUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.957" />

    <Button
        android:id="@+id/adminModUser"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Modificar Usuario"
        app:layout_constraintBottom_toTopOf="@+id/adminDeleteUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.961" />

    <Button
        android:id="@+id/adminDeleteUser"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:text="Eliminar Usuario"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.822" />
</androidx.constraintlayout.widget.ConstraintLayout>